    <CALCULATION>
        <ANALYSIS>C_HWC_V</ANALYSIS>
        <COMPONENT>HWC.OC</COMPONENT>
        <VERSION>7</VERSION>
        <DESCRIPTION></DESCRIPTION>
        <SOURCE_CODE>&apos; Invoeren van resultaat gemeten in mg/l omrekenen naar mg/kg&apos;&#10;&apos; Formule: mg C/kg = mg/l C gemeten * 25 / Gbodem = mg/l C * VDextractie&#10;&#10;&apos; Breakpoint(aReason) &#10;&apos; sampletype = select sample.sample_type &apos;we hebben eigenlijk QC sample type nodig !!!!!!  --&gt; halen uit QC_SAMPLES.QC_TYPE&#10;&#10;&apos; &apos; &apos;GERRIT, hieronder staat de code om op sampletype te zoeken, om uit te testen als je weer een staal hebt met deze analyse&#10;&apos; sampleType = select sample.sample_type&#10;&apos; if (not(isNull(sampleType))) then&#10;&apos; &#9;sampleNumber = select sample.sample_number&#10;&apos; &#9;q = &quot;SELECT QC_SAMPLES.QC_TYPE from SAMPLE inner join QC_SAMPLES on SAMPLE.PRODUCT_GRADE = QC_SAMPLES.NAME where SAMPLE_NUMBER = &quot; + sample_number&#10;&apos; &#9;status = sql(q, &quot;qctypeArray&quot;)&#10;&apos; &#9;if (qctypeArray[1,1] = &quot;CONTROL&quot;) then&#10;&apos; &#9;&#9;RETURN&#10;&apos; &#9;endif&#10;&apos; endif&#10;&#10;&apos; if (isEmpty(VDextractie)) then &#9;&apos;via isEmpty verhinderen dat de berekening doorgaat indien de resultaten van voorbehandeling niet zijn ingevuld&#10;&apos; &#9;if (sampleType = &quot;STANDARD&quot;) then&#10;&apos; &#9;&#9;VDextractie = 1&#10;&apos;     else&#10;&apos;         RETURN&#10;&apos;     endif&#10;&apos; endif&#10;&#10;HWC = OC * VDextractie&#10;return HWC&#10;&#10;&#10;&#10;&#10;</SOURCE_CODE>
    </CALCULATION>

