    <CALCULATION>
        <ANALYSIS>C_TIC_ANALYSER</ANALYSIS>
        <COMPONENT>dummyCalc</COMPONENT>
        <VERSION>8</VERSION>
        <DESCRIPTION></DESCRIPTION>
        <SOURCE_CODE>&apos;Berekening TIC wanneer de pH kleiner is dan 6.5&apos; --&gt; Niet uitvoeren, analyse cancelen&#10;&#10;RETURN 0&#10;&#10;&apos;De code hieronder wordt op dit moment niet meer gebruiktm de trigger gebeurt nu op phKCL_VOL_T90_V&#10;&apos; de trigger hier werkt niet omdat de pH niet aan het moederstaal herkend wordt&#10;&apos; laten triggeren zonder pH via always calculate leidt tot veel onnodig rekenwerk&#10;&#10;Breakpoint(aReason) &#10;MsgBox(&quot; here &quot; )&#10;&#10;testnumber = SELECT RESULT.TEST_NUMBER&#10;sampleNumber  = SELECT RESULT.SAMPLE_NUMBER&#10;sampleId = SELECT SAMPLE.SAMPLE_ID&#10;&#10;if (PH01 &lt; 6.5) then&#10;&#9;&apos;canceltest&#10;&#9;status = CancelTest(sampleNumber,testNumber) &#10;&#9;&apos;status = RemoveTest(sampleId, testNumber) &#10;&#9;return -1&#10;else &#10;&#9;return 1&#10;&#9;&#10;endif &#10;&#10;&#10;&#10;&apos; NOG OUDERE VERSIE&#10;&#10;&apos;pH analyse = PHKCL_VOL_T90_V&#10;&apos;TIC analyse = C_TIC_ANALYSER&#10;&apos;Wanneer de pH kleiner is dan 6.5 wordt de TIC metingen niet uitgevoerd&#10;&#9;&apos; De component &apos;TIC_%&apos; van de TIC analyse krijgt als default value &apos;0&apos;&#10;&#9;&apos; De component &apos;Gewicht&apos; krijgt als default value &apos;500&apos; (500 mg)&#10;&#9;&apos; De component &apos;TIC_%&apos; wordt omgegerekend met de DS naar &apos;T.IC.DS&apos;&#10;&#10;&apos;breakpoint()&#10;&#10;&#10;&apos; testNumber = SELECT RESULT.TEST_NUMBER&#10;&apos; sampleNumber = select RESULT.SAMPLE_NUMBER&#10;&apos; originalSample = select sample.original_sample&#10;&#10;&#10;&apos; resultNumberTIC = SELECT RESULT.RESULT_NUMBER&#10;&apos; &#9;&#9;&#9;WHERE NAME = &quot;TIC_%&quot;&#10;&apos; &#9;&#9;&#9;AND TEST_NUMBER = testNumber&#10;&apos; &#9;&#9;&#9;ORDER BY ANALYSIS&#10;&#10;&apos; resultNumberTICDS = SELECT RESULT.RESULT_NUMBER&#10;&apos; &#9;&#9;&#9;WHERE NAME = &quot;T.IC.DS&quot;&#10;&apos; &#9;&#9;&#9;AND TEST_NUMBER = testNumber&#10;&apos; &#9;&#9;&#9;ORDER BY ANALYSIS&#10;&#10;&apos; resultNumberGew = SELECT RESULT.RESULT_NUMBER&#10;&apos; &#9;&#9;&#9;WHERE NAME = &quot;Gewicht&quot;&#10;&apos; &#9;&#9;&#9;AND TEST_NUMBER = testNumber&#10;&apos; &#9;&#9;&#9;ORDER BY ANALYSIS&#10;&#10;&apos; sampletype = select sample.sample_type&#10;&apos; &#9;where sample_number = originalSample&#10;&apos; &#9;order by sample_number&#10;&#10;&apos; Breakpoint(aReason) &#10;&apos; if (pH01 &lt; 6.5) then &#10;&apos; &#9;dmc = 2&#10;&apos; &#9;gewicht = 500&#10;&apos; &#9;koolstof = 0&#10;&apos; &#9;status = SetResult(sampleNumber,testNumber,resultnumberGew, gewicht,&quot;T&quot;)&#10;&apos; &#9;status = SetResult(sampleNumber,testNumber,resultnumberTIC, koolstof, &quot;T&quot;)&#10;&#9;&#10;&#9;&apos; else  &#10;&#9;&apos; &#9;dmc = 1&#10;&#9;&apos; endif &#10;&#10;&apos; &apos;Omrekening naar droge stof&#10;&apos; &apos;---------------------------&#10;&#10;&apos; save() &apos;tussentijdste save stap nodig zodat de database en context dezelfde info bevatten&#10;&#10;&apos; if (dmc = 1) then&#10;&apos; &#9;koolstof  = SELECT RESULT.ENTRY&#10;&apos; &#9;&#9;&#9;&#9;WHERE RESULT_NUMBER = resultNumberTIC &#10;&apos; &#9;&#9;&#9;&#9;ORDER BY ENTRY&#10;&apos; &#9;if (IsNull(koolstof)) then&#10;&apos; &#9;&#9;dmc = 3&#10;&apos; &#9;&#9;return dmc &apos;ga uit de routine, DS kan niet berekend worden als er geen TIC_%  bestaat&#10;&apos; &#9;else &#10;&apos; &#9;&#9;koolstof = val(koolstof)&#10;&apos; &#9;endif &#10;&apos; endif&#10;&#10;&apos; Subroutine(&quot;FN_ANA_DS_CHOICE&quot;)  &apos;zet de variabele AnalysisName (ipv &apos;AnalysisName = &quot;DS_OVEN_105&quot;)&#10;&apos; AnalysisComp = &quot;DS.105&quot; &#10;&#10;&apos; DS105 = select result.numeric_entry&#10;&apos; &#9;&#9;&#9;where sample_number = originalSample&#10;&apos; &#9;&#9;&#9;and analysis = AnalysisName &#10;&apos; &#9;&#9;&#9;and name = AnalysisComp&#10;&apos; &#9;&#9;&#9;order by sample_number&#10;&#10;&apos; if (isNull(sampletype) or (sampletype = &quot;DUP&quot;)) then&#10;&apos; &#9;if (isEmpty(DS105)) then&#10;&apos; &#9;&#9;dmc = -1&#10;&apos; &#9;&#9;RETURN dmc &apos;return -1 as dummycalc&#10;&apos; &#9;endif&#10;&apos; else&#10;&apos; &#9;Subroutine(&quot;FN_COMPCALC_DS_CORRECT&quot;) &apos;returns dscorrectie &#10;&apos; &#9;DS105 = 1000 * dscorrectie&#10;&apos; endif&#10;&#10;&apos; KoolstofDS = (Koolstof / DS105) * 10000&#10;&apos; status = SetResult(sampleNumber,testNumber,resultNumberTICDS ,KoolstofDS ,&quot;T&quot;)&#10;&#10;&apos; Return dmc&#10;&#10;&#10;&apos; &apos; omrekenen naar droge stof&#10;&apos;INPUT VAR: Koolstof = TIC_%&#10;&#10;&apos; originalSample = select sample.original_sample&#10;&#10;&apos; sampletype = select sample.sample_type&#10;&apos; &#9;&#9;&#9;where sample_number = originalSample&#10;&apos; &#9;&#9;&#9;order by sample_number&#10;&#10;&apos; AnalysisName = &quot;DS_OVEN_105&quot; &#10;&apos; AnalysisComp = &quot;DS.105&quot; &#10;&#10;&apos; DS105 = select result.numeric_entry&#10;&apos; &#9;&#9;&#9;where sample_number = originalSample&#10;&apos; &#9;&#9;&#9;and analysis = AnalysisName &#10;&apos; &#9;&#9;&#9;and name = AnalysisComp&#10;&apos; &#9;&#9;&#9;order by sample_number&#10;&#10;&apos; if (isNull(sampletype) or (sampletype = &quot;DUP&quot;)) then&#10;&apos; &#9;if (isEmpty(DS105)) then&#10;&apos; &#9;&#9;RETURN&#10;&apos; &#9;endif&#10;&apos; else&#10;&apos; &#9;Subroutine(&quot;FN_COMPCALC_DS_CORRECT&quot;) &apos;returns dscorrectie &#10;&apos; &#9;DS105 = 1000 * dscorrectie&#10;&apos; endif&#10;&#10;&apos; KoolstofDS = (Koolstof / DS105) * 10000&#10;&apos; Return KoolstofDS&#10;&#10;</SOURCE_CODE>
    </CALCULATION>

