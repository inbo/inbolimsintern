    <CALCULATION>
        <ANALYSIS>PHKCL_VOL_T90_V</ANALYSIS>
        <COMPONENT>cancel_C_TIC_ANALYSER</COMPONENT>
        <VERSION>2</VERSION>
        <DESCRIPTION></DESCRIPTION>
        <SOURCE_CODE>&apos;cancel alle lege C_TIC_ANALYSER voor dit staal en bijhorende aliquots&#10;Breakpoint(aReason) &#10;origsample = select SAMPLE.ORIGINAL_SAMPLE&#10;&#10;if (pHval &lt; 6.5) then&#10;&#9;q = &quot;SELECT s.SAMPLE_NUMBER, t.TEST_NUMBER from TEST t inner join SAMPLE s on t.SAMPLE_NUMBER = s.SAMPLE_NUMBER &quot; &#10;&#9;q = q + &quot; where t.status = &apos;I&apos; and t.ANALYSIS = &apos;C_TIC_ANALYSER&apos; and s.ORIGINAL_SAMPLE = &quot; + origsample&#10;&#9;status = SQL(q,  &quot;cancelarray&quot;)&#10;&#9;nrw = Ubound(cancelarray, 1)&#10;&#9;valc = 0&#10;&#9;if (nrw &gt; 0) then&#10;&#9;&#9;for i = 1 to nrw &#10;&#9;&#9;&#9;snum = cancelarray[i, 1]&#10;&#9;&#9;&#9;tnum = cancelarray[i, 2]&#10;&#9;&#9;&#9;status = CancelTest(snum, tnum) &#10;&#9;&#9;&#9;valc = valc + 1&#10;&#9;&#9;next i&#10;&#9;&#9;&apos;save()&#10;&#9;&#9;return valc&#10;&#9;else &#10;&#9;&#9;return 0&#10;&#9;endif&#10;&#10;else &#10;&#9;return 0&#10;&#9;&#10;endif &#10;&#10;</SOURCE_CODE>
    </CALCULATION>

