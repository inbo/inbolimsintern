    <CALCULATION>
        <ANALYSIS>DNA_PLREAD</ANALYSIS>
        <COMPONENT>Hoeveelheid DNA</COMPONENT>
        <VERSION>2</VERSION>
        <DESCRIPTION></DESCRIPTION>
        <SOURCE_CODE>sampNum = SELECT SAMPLE.SAMPLE_NUMBER&#10;&#10;if (Conc = 0) then&#10;    RETURN 0&#10;endif&#10;&#10;if (USER = &quot;PIETERVS&quot;) then&#10;&#9;&apos;    breakpoint()&#10;endif&#10;&#10;&#10;q = &quot;select  ENTRY FROM RESULT r inner join ANALYSIS a on a.NAME = r.ANALYSIS&quot;&#10;q = q + chr(10) + &quot;WHERE r.SAMPLE_NUMBER = &quot; + sampNum&#10;q = q + chr(10) + &quot; AND ANALYSIS_TYPE = &apos;EXTRACTIE&apos; &quot;&#10;q = q + chr(10) + &quot;AND r.NAME = &apos;Volume_eluaat&apos; &quot;&#10;&#10;status = SQL(q, &quot;ValuesArray&quot;)&#10;&#10;waarde = ValuesArray[1,1]&#10;status = IsNumeric(waarde)&#10;if (status = true) then&#10;    waarde = Val(waarde)&#10;    berekening = Conc * waarde&#10;    RETURN berekening&#10;else&#10;    RETURN&#10;endif&#10;</SOURCE_CODE>
    </CALCULATION>

