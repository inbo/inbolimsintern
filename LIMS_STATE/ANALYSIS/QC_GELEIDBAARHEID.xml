    <CALCULATION>
        <ANALYSIS>QC_GELEIDBAARHEID</ANALYSIS>
        <COMPONENT>EC.CALC</COMPONENT>
        <VERSION>3</VERSION>
        <DESCRIPTION></DESCRIPTION>
        <SOURCE_CODE>&#10;&#10;IF (IsEmpty(NA)) THEN&#10;    NA = 0&#10;ENDIF&#10;IF (IsEmpty(K)) THEN&#10;    K = 0&#10;ENDIF&#10;IF (IsEmpty(MG)) THEN&#10;    MG = 0&#10;ENDIF&#10;IF (IsEmpty(CA)) THEN &#10;    CA = 0&#10;ENDIF&#10;IF (IsEmpty(NH4)) THEN&#10;    NH4 = 0&#10;ENDIF&#10;IF (IsEmpty(H)) THEN&#10;    H = 0&#10;ENDIF&#10;IF (IsEmpty(FE)) THEN&#10;    FE = 0&#10;ENDIF&#10;IF (IsEmpty(AL)) THEN&#10;    AL = 0&#10;ENDIF&#10;IF (IsEmpty(MN)) THEN&#10;    MN = 0&#10;ENDIF&#10;IF (IsEmpty(CL)) THEN&#10;    CL = 0&#10;ENDIF&#10;IF (IsEmpty(NO2)) THEN&#10;    NO2 = 0&#10;ENDIF&#10;IF (IsEmpty(NO3)) THEN&#10;    NO3 = 0&#10;ENDIF&#10;IF (IsEmpty(SO4)) THEN&#10;    SO4 = 0&#10;ENDIF&#10;IF (IsEmpty(PO4)) THEN&#10;    PO4 = 0&#10;ENDIF&#10;IF (IsEmpty(HCO3)) THEN&#10;    HCO3 = 0&#10;ENDIF&#10;IF (IsEmpty(CO3)) THEN&#10;    CO3 = 0&#10;ENDIF&#10;IF (IsEmpty(OH)) THEN&#10;    OH = 0&#10;ENDIF&#10;IF (IsEmpty(RCOO)) THEN&#10;    RCOO = 0&#10;ENDIF&#10;&#10;&#10;&#10;isNa = 1 * NA&#10;isK = 1 * K&#10;isMG = 2 * MG&#10;isCA = 2 * CA&#10;isNH4 = 1 * NH4&#10;isH = 1 * H&#10;isFE = 2 * FE&#10;isAL = 2 * AL&#10;isMN = 2 * MN&#10;isCL = 1 * CL&#10;isNO2 = 1 * NO2&#10;isNO3 = 1 * NO3&#10;isSO4 = 2 * SO4&#10;isPO4 = 3 * PO4&#10;isHCO3 = 1 * HCO3&#10;isCO3 = 2 * CO3&#10;isOH = 1 * OH&#10;isRCOO = 1 * RCOO&#10;&#10;&apos;Calculate the IS sum&#10;isSOM = 0.5 * ((isNa + isK + isMG + isCA + isNH4 + isH + isFE + isAL + isMN + isCL + isNO2 + isNO3 + isSO4 + isPO4 + isHCO3 + isCO3 + isOH + isRCOO) / 1000)&#10;&#10;&apos;Calculate the activity coefficients (z)&#10;power = -1 * (Sqr(isSOM) / (1 + sqr(isSOM) - (0.2 * isSOM)) * 0.5085 * (1 ^ 2))&#10;Z1 = 10 ^ power&#10;&#10;power = -1 * (Sqr(isSOM) / (1 + sqr(isSOM) - (0.2 * isSOM)) * 0.5085 * (2 ^ 2))&#10;Z2 = 10 ^ power&#10;&#10;power = -1 * (Sqr(isSOM) / (1 + sqr(isSOM) - (0.2 * isSOM)) * 0.5085 * (3 ^ 2))&#10;Z3 = 10 ^ power&#10;&#10;ecNa = 1000 * (0.0501 * Z1 * NA)&#10;ecK = 1000 * (0.0735 * Z1 * K)&#10;ecMG = 1000 * (0.0531 * Z2 * MG)&#10;ecCA = 1000 * (0.0595 * Z2 * CA)&#10;ecNH4 = 1000 * (0.0735 * Z1 * NH4)&#10;ecH = 1000 * (0.350 * Z1 * H)&#10;ecFE = 1000 * (0.068 * Z2 * FE)&#10;ecAL = 1000 * (0.0665 * Z2 * AL)&#10;ecMN = 1000 * (0.065 * Z2 * MN)&#10;ecCL = 1000 * (0.0764 * Z1 * CL)&#10;ecNO2 = 1000 * (0 * Z1 * NO2)&#10;ecNO3 = 1000 * (0.0714 * Z1 * NO3)&#10;ecSO4 = 1000 * (0.08 * Z2 * SO4)&#10;ecPO4 = 1000 * (0 * Z3 * PO4)&#10;ecHCO3 = 1000 * (0.0445 * Z1 * HCO3)&#10;ecCO3 = 1000 * (0 * Z2 * CO3)&#10;ecOH = 1000 * (0 * Z1 * OH)&#10;ecRCOO = 1000 * (0 * Z1 * RCOO)&#10;&#10;ECcalculated = ecNa + ecK + ecMG + ecCA + ecNH4 + ecH + ecFE + ecAL + ecMN + ecCL + ecNO2 + ecNO3 + ecSO4 + ecPO4 + ecHCO3 + ecCO3 + ecOH + ecRCOO&#10;&#10;Return ECcalculated&#10;&#10;</SOURCE_CODE>
    </CALCULATION>

