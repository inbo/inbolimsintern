    <CALCULATION>
        <ANALYSIS>CACO3_TIT_V</ANALYSIS>
        <COMPONENT>CaCO3%</COMPONENT>
        <VERSION>5</VERSION>
        <DESCRIPTION></DESCRIPTION>
        <SOURCE_CODE>&apos;Berekening percentage CaCO3&apos;&#10;&#10;&apos;Als geen pH analyse aanwezig is, stel dan de pH = 6 zodat toch een berekening uitgevoerd wordt.&#10;&apos;Als de pH kleiner is dan 6, dan is CaCO3= 0&#10;&apos;Berekenen van de pH uit 2 verschillende testen&#10;&#10;&apos;breakpoint()&#10;&#10;if (notEmpty(pH01)) then&#10;    pH =  pH01&#10;elseif (notEmpty(pH02)) then&#10;    pH = pH02&#10;elseif (notEmpty(pHEC)) then&#10;    pH = pHEC&#10;else&#10;    pH = 8 &apos;gewoon groter dan 6 willekeurig&#10;endif&#10;&#10;if (pH &lt; 6) then&#10;    CaCO3 = 0&#10;    testNumber = SELECT RESULT.TEST_NUMBER&#10;    sampleNumber = select RESULT.SAMPLE_NUMBER&#10;&#10;    resultNumber1 = SELECT RESULT.RESULT_NUMBER&#10;&#9;WHERE NAME = &quot;Titer&quot;&#10;    AND TEST_NUMBER = testNumber&#10;    ORDER BY ANALYSIS&#10;&#10;    resultNumber2 = SELECT RESULT.RESULT_NUMBER&#10;&#9;WHERE NAME = &quot;Gewicht&quot;&#10;    AND TEST_NUMBER = testNumber&#10;    ORDER BY ANALYSIS&#10;&#10;    resultNumber3 = SELECT RESULT.RESULT_NUMBER&#10;    WHERE NAME = &quot;V_staal&quot;&#10;    AND TEST_NUMBER = testNumber&#10;    ORDER BY ANALYSIS&#10;&#10;    resultNumber4 = SELECT RESULT.RESULT_NUMBER&#10;    WHERE NAME = &quot;V_blanco&quot;&#10;    AND TEST_NUMBER = testNumber&#10;    ORDER BY ANALYSIS&#10;&#10;&#9;resultNumber5 = SELECT RESULT.RESULT_NUMBER&#10;&#9;WHERE NAME = &quot;Reagens&quot;&#10;&#9;AND TEST_NUMBER = testNumber&#10;&#9;ORDER BY ANALYSIS&#10;&#9;&#10;&#9;resultNumber6 = SELECT RESULT.RESULT_NUMBER&#10;    WHERE NAME = &quot;CaCO3.DS&quot;&#10;    AND TEST_NUMBER = testNumber&#10;    ORDER BY ANALYSIS&#10;&#10;&#9;status = SetResult(sampleNumber,testNumber, resultnumber1, 0.5,&quot;T&quot;)&#10;    status = SetResult(sampleNumber,testNumber, resultnumber2, 1,&quot;T&quot;)&#10;    status = SetResult(sampleNumber,testNumber, resultnumber3, 25,&quot;T&quot;)&#10;    status = SetResult(sampleNumber,testNumber, resultnumber4, 25,&quot;T&quot;)&#10;&#9;status = SetResult(sampleNumber,testNumber, resultnumber5, &quot;NONE&quot;,&quot;T&quot;)&#10;&#9;ans = CalculateResult(resultNumber6)&#10;    &#10;    Return CaCO3&#10;Endif&#10;&#10;if ((isEmpty(TIT) OR isEmpty(VBL)) or (isEmpty(VST) or isEmpty(Gewicht))) then&#10;    return&#10;endif&#10;&#10;CACO3 = (VBL - VST) * TIT * 50 / Gewicht / 10&#10;return CACO3&#10;&#10;&#10;</SOURCE_CODE>
    </CALCULATION>

